{
  "name": "Grzegorz's Site Blocker",
  "description": "This extension blocks unwanted sites.",
  "author": "Grzegorz Milka",
  "version": "1.17.0",
  "manifest_version": 3,
  "icons": {
    "128": "g_128.png"
  },
  "permissions": ["declarativeNetRequest", "storage", "tabs"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "g_128.png",
    "default_popup": "options.html"
  },
  "options_page": "options.html"
}
